{
    "manifest_version": 2,
    "name": "Censorus",
    "description": "Lorem ipsum dolor sit amet consectetur adipisicing elit. Explicabo tempore repudiandae voluptas ad iure beatae eveniet impedit voluptates, sapiente consectetur.",
    "version": "1.0.0",
    "icons": {"128": "/images/logo.svg" },
    "browser_action": {
       "default_icon": "/images/logo.svg",
       "default_title": "Censorus",
       "default_popup": "popup.html"
    },
    "background": {
       "scripts": ["./background.js"]
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["script.js"],
        "all_frames": true
      }
    ],
    "permissions": [ "activeTab", "http://*/*", "https://*/*","storage" ]
 }
 